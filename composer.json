{
    "name": "cellear/wp2bd",
    "description": "WordPress Theme Compatibility Layer for Backdrop CMS",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "backdrop/backdrop",
                "version": "1.30.0",
                "type": "backdrop-core",
                "dist": {
                    "url": "https://github.com/backdrop/backdrop/archive/refs/tags/1.30.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^2.0",
        "johnpbloch/wordpress-core-installer": "^2.0",
        "backdrop/backdrop": "1.30.0",
        "johnpbloch/wordpress-core": "4.9"
    },
    "extra": {
        "installer-paths": {
            "ext-backdrop/": ["backdrop/backdrop"],
            "ext-wordpress/": ["johnpbloch/wordpress-core"]
        },
        "wordpress-install-dir": "ext-wordpress"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "johnpbloch/wordpress-core-installer": true
        }
    }
}
